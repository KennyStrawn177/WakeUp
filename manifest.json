{
  "manifest_version": 2,
  "name": "Wake Up!",
  "description": "Proof-of-concept packaged app for keeping device from going to sleep",
  "version": "6.2.4",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "main.js"
      ]
    }
  },
  "permissions": [
    "power",
    "notifications",
    "storage",
    "unlimitedStorage"
  ]
}
